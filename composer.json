{
  "name": "valiant-bedrock/essence",
  "description": "PMMP plugin to implement essential features for the network.",
  "type": "project",
  "version": "0.5.0",
  "require": {
    "php": ">=8.2",
    "valiant-bedrock/libcommand": "dev-legacy/pm4",
    "valiant-bedrock/libdiscord": "dev-legacy/pm4",
    "sylvrs/libmarshal": ">=1.4.3",
    "sof3/await-generator": ">=3.5.2"
  },
  "require-dev": {
    "phpstan/phpstan": "1.10.15",
    "friendsofphp/php-cs-fixer": "v3.17.0",
    "pocketmine/pocketmine-mp": "dev-legacy/pm4"
  },
  "scripts": {
    "analyze": "./vendor/bin/phpstan analyze",
    "run-cs-fixer": "./vendor/bin/php-cs-fixer fix --diff --ansi",
    "generate-translations": "php ./scripts/generate_translations.php"
  },
  "license": "proprietary",
  "authors": [
    {
      "name": "sylvrs",
      "email": "matthew@valiant.watch"
    }
  ],
  "minimum-stability": "dev",
  "repositories": [
    { "type": "vcs", "url": "git@github.com:NetherGamesMC/PocketMine-MP.git" },
    { "type": "vcs", "url": "git@github.com:NetherGamesMC/BedrockProtocol.git" },
    { "type": "vcs", "url": "git@github.com:NetherGamesMC/BedrockData.git" },
    { "type": "vcs", "url": "git@github.com:Valiant-Bedrock/libcommand.git" },
    { "type": "vcs", "url": "git@github.com:Valiant-Bedrock/libDiscord.git" }
  ]
}
